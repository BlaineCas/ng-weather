{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "WeatherAPI",
      "urls": [
        "https://api.openweathermap.org/data/2.5/**"
      ],
      "cacheConfig": {
        "maxAge": "2h",
        "maxSize": 10000,
        "strategy": "performance"
      }
    }]
}